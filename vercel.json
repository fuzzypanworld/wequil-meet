{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        },
        {
            "src": "www/**",
            "use": "@vercel/static"
        },
        {
            "src": "assets/**",
            "use": "@vercel/static"
        },
        {
            "src": "node_modules/vue/dist/vue.global.prod.js",
            "use": "@vercel/static"
        },
        {
            "src": "node_modules/webrtc-adapter/out/adapter.js",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/socket.io/(.*)",
            "dest": "server.js"
        },
        {
            "src": "/favicon.svg",
            "dest": "/assets/favicon.svg"
        },
        {
            "src": "/favicon.ico",
            "dest": "/assets/favicon.ico"
        },
        {
            "src": "/apple-touch-icon.png",
            "dest": "/assets/apple-touch-icon.png"
        },
        {
            "src": "/mask-icon.svg",
            "dest": "/assets/mask-icon.svg"
        },
        {
            "src": "/vue.global.prod.js",
            "dest": "/node_modules/vue/dist/vue.global.prod.js"
        },
        {
            "src": "/adapter-latest.js",
            "dest": "/node_modules/webrtc-adapter/out/adapter.js"
        },
        {
            "src": "/assets/(.*)",
            "dest": "/assets/$1"
        },
        {
            "src": "/www/(.*)",
            "dest": "/www/$1"
        },
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
} 